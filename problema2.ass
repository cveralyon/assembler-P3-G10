DATA:
    inicio #A0
    largo 5
    res 1
    mem1 0
    mem2 0
    valor1 0
CODE:
    MOV B,(inicio)
    MOV (mem1),B
    ADD B,largo
    MOV (mem2),B
comparar:
    MOV B,(mem1)
    MOV A,(B)
    MOV (valor1),A
    MOV B,(mem2)
    MOV A,(B)
    CMP A,(valor1)
    JNE end
    INC mem1
    SUB mem2,1
    JMP comparar
end:
    res 0