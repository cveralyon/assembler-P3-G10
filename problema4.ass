DATA:
    inicio #A0
    largo 5
    valor1 0
    aux1 0
    mem1 #A0
    mem2 #A0
CODE:
    MOV B,largo
    MOV (aux1),B
    MOV B,(inicio)
subir:
    MOV (mem1),B
    INC B
    MOV (mem2),B
    MOV A,(B)
    MOV (valor1),A
    MOV B,(mem1)
    MOV A,(B)
    CMP A,(valor1)
    JLE siguiente1
    MOV B,largo
    MOV (aux1),B
    MOV B,valor1
    MOV (mem1),B
    MOV (mem2),A
    INC mem1
    INC mem2
    JMP subir
siguiente1:
    MOV B,(aux1)
    SUB B,1
    CMP B,1
    JNE seguir
    JMP end
seguir:
    MOV (aux1),B
    INC mem1
    INC mem2
    JMP subir
end:  