DATA:
    inicio #A0
    largo 5
    valor1 0
    aux1 0
    mem1 #00
    mem2 #00
CODE:
    MOV B,largo
    MOV (aux1),B
    MOV B,(inicio)
subir:
    MOV (mem1),B // #A0 aqui esta A
    INC B
    MOV (mem2),B // #A1 aqui esta valor1
    MOV A,(B)
    MOV (valor1),A
    MOV B,(mem1)
    MOV A,(B)
    CMP A,(valor1) // si A mayor a valor1 se intercambia
    JLE siguiente1
    MOV B,largo
    MOV (aux1),B
    MOV (mem1),valor1 // en la memoria donde estaba A pongo valor1
    MOV (mem2),A  // en la memoria donde estaba valor1 pongo A
    MOV mem1,mem2
    INC mem2 // #A2
    JMP subir
siguiente1:
    MOV B,(aux1)
    SUB B,1
    CMP B,1
    JNE seguir
    JMP end
seguir:
    MOV (aux1),B
    INC mem1
    INC mem2
    JMP subir
end:





